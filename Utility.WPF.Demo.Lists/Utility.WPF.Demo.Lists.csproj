<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<TargetFramework>net6.0-windows</TargetFramework>
		<Nullable>enable</Nullable>
		<UseWPF>true</UseWPF>
	</PropertyGroup>

	<ItemGroup>
		<ProjectReference Include="..\Utility.ViewModels\Utility.ViewModels.csproj" />
		<ProjectReference Include="..\Utility.WPF.Controls.Lists\Utility.WPF.Controls.Lists.csproj" />
		<ProjectReference Include="..\Utility.WPF.Converters\Utility.WPF.Converters.csproj" />
		<ProjectReference Include="..\Utility.WPF.Markup\Utility.WPF.Markup.csproj" />
		<ProjectReference Include="..\Utility.WPF.Meta\Utility.WPF.Meta.csproj" />
		<ProjectReference Include="..\Utility.WPF.Controls\Utility.WPF.Controls.csproj" />
		<ProjectReference Include="..\Utility.WPF.Demo.Data\Utility.WPF.Demo.Data.csproj" />
	</ItemGroup>

	<ItemGroup>
		<Page Update="ButtonUserControl.xaml">
			<XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
			<SubType>Designer</SubType>
		</Page>
		<Page Update="ButtonsUserControl.xaml">
			<XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
			<SubType>Designer</SubType>
		</Page>
		<Page Update="CheckBoxesUserControl.xaml">
		  <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
		  <SubType>Designer</SubType>
		</Page>
		<Page Update="DualButtonUserControl.xaml">
			<XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
			<SubType>Designer</SubType>
		</Page>
		<Page Update="Infrastructure\DataTemplates.xaml">
		  <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
		  <SubType>Designer</SubType>
		</Page>
	</ItemGroup>

	<ItemGroup>
		<Compile Update="ButtonsUserControl.xaml.cs">
			<SubType>Code</SubType>
		</Compile>
		<Compile Update="CheckBoxesUserControl.xaml.cs">
		  <SubType>Code</SubType>
		</Compile>
	</ItemGroup>

</Project>

                <!--  Subtext  -->
                <!--
                <TextBlock Text="Sum of all active loans"
                   FontSize="12"
                   Foreground="#6C757D"
                   FontStyle="Italic"
                   Margin="0,4,0,0"/>-->
            </StackPanel>
        </Border>
    </DataTemplate>


    <!--  Value Change Converter  -->



    <DataTemplate x:Key="PriceTemplate" DataType="{x:Type model:Model}">
        <Border
            x:Name="MainBorder"
            Margin="10"
            Padding="16,12"
            Background="#F8F9FA"
            BorderBrush="#E9ECEF"
            BorderThickness="1"
            CornerRadius="8"
            DataContext="{Binding Value}"
            Tag="{Binding Converter={StaticResource ValueChangeConverter}}">
            <Border.Style>
                <Style TargetType="Border">
                    <Style.Triggers>
                        <!--  Green flash for increase  -->
                        <DataTrigger Binding="{Binding Path=Tag, RelativeSource={RelativeSource Mode=Self}}" Value="{x:Static local:ChangeType.Increase}">
                            <DataTrigger.EnterActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <ColorAnimation
                                            Storyboard.TargetProperty="Background.(SolidColorBrush.Color)"
                                            From="#90EE90"
                                            To="#F8F9FA"
                                            Duration="0:0:1.5" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </DataTrigger.EnterActions>
                            <DataTrigger.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <ColorAnimation
                                            Storyboard.TargetProperty="Background.(SolidColorBrush.Color)"
                                            From="#FFB3B3"
                                            To="#F8F9FA"
                                            Duration="0:0:1.5" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </DataTrigger.ExitActions>
                        </DataTrigger>

                        <!--  Red flash for decrease  -->
                        <!--<DataTrigger Binding="{Binding Path=Tag,RelativeSource={RelativeSource Mode=Self}}" Value="{x:Static local:ChangeType.Decrease}">

                        </DataTrigger>-->
                    </Style.Triggers>
                </Style>
            </Border.Style>

            <StackPanel>
                <!--  Header  -->
                <!--<TextBlock Text="SUM"
                       FontSize="14"
                       FontWeight="SemiBold"
                       Foreground="#495057"
                       Margin="0,0,0,8"/>-->

                <!--  Loan Amount Display with Change Indicator  -->
                <StackPanel Margin="0,0,0,4" Orientation="Horizontal">
                    <TextBlock
                        x:Name="TotalLoansTextBlock"
                        HorizontalAlignment="Left"
                        FontSize="24"
                        FontWeight="Bold"
                        Foreground="#1A237E"
                        Text="{Binding StringFormat='£{0:N2}', ConverterCulture=en-GB}" />

                    <!--  Change Arrow  -->
                    <Polygon Margin="8,6,0,0" VerticalAlignment="Center">
                        <Polygon.Style>
                            <Style TargetType="Polygon">
                                <!--<Setter Property="Visibility" Value="Collapsed"/>-->
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding ElementName=MainBorder, Path=Tag}" Value="{x:Static local:ChangeType.Increase}">
                                        <Setter Property="Fill" Value="#28A745" />
                                        <Setter Property="Visibility" Value="Visible" />
                                        <Setter Property="Points" Value="0,0 5,-5 10,0" />
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding ElementName=MainBorder, Path=Tag}" Value="{x:Static local:ChangeType.Decrease}">
                                        <Setter Property="Fill" Value="#DC3545" />
                                        <Setter Property="Visibility" Value="Visible" />
                                        <Setter Property="Points" Value="0,0 5,5 10,0" />
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Polygon.Style>
                    </Polygon>
                </StackPanel>

                <!--  Subtext  -->
                <!--
            <TextBlock Text="Sum of all active loans"
                       FontSize="12"
                       Foreground="#6C757D"
                       FontStyle="Italic"
                       Margin="0,4,0,0"/>-->
            </StackPanel>
        </Border>
    </DataTemplate>


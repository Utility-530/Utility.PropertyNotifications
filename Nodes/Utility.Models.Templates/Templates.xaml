    <DataTemplate x:Shared="false" DataType="{x:Type inf:DataFileModel}">
        <Grid Name="rootElement" Background="#DCDCDC">
            <Grid.Resources>
                <Style TargetType="{x:Type TextBox}">
                    <Setter Property="Margin" Value="10,5,10,0" />
                </Style>
            </Grid.Resources>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
            </Grid.ColumnDefinitions>
            <!--<Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>-->


            <TextBlock
                Name="nameLabel"
                Grid.Column="0"
                Style="{StaticResource inlineText}">
                File Path
            </TextBlock>


            <TextBlock
                Name="txtAddress"
                Grid.Row="2"
                Grid.Column="1"
                HorizontalAlignment="Left"
                VerticalAlignment="Bottom"
                FontWeight="DemiBold"
                Text="{Binding FilePath}" />

            <TextBlock
                Name="aliasLabel"
                Grid.Column="2"
                Style="{StaticResource inlineText}">
                File Name
            </TextBlock>
            <TextBlock
                Name="txtName"
                Grid.Column="3"
                Width="50"
                HorizontalAlignment="Left"
                VerticalAlignment="Bottom"
                FontWeight="DemiBold"
                Text="{Binding FileName}" />
            <TextBlock
                Name="alia3sLabel"
                Grid.Column="4"
                Style="{StaticResource inlineText}">
                Model Name
            </TextBlock>
            <TextBlock
                Name="txtName2"
                Grid.Column="5"
                Width="100"
                HorizontalAlignment="Left"
                VerticalAlignment="Bottom"
                FontWeight="DemiBold"
                Text="{Binding TableName}" />
        </Grid>
    </DataTemplate>




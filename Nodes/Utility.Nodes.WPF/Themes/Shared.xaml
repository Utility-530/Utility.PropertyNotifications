                    xmlns:mt="clr-namespace:Utility.Models.Templates;assembly=Utility.Models.Templates">



    <local:SelectedNodeChangeConverter x:Key="SelectedNodeChangeConverter"/>
    <conv:TypeNameConverter x:Key="TypeNameConverter"/>

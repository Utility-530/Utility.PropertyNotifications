	</ItemGroup>

	<ItemGroup>
		<Compile Update="SplitControl.cs">
			<DependentUpon>SplitControl.xaml</DependentUpon>
		</Compile>	
		<Compile Update="SplitItemsControl.cs">
			<DependentUpon>SplitItemsControl.xaml</DependentUpon>
		</Compile>
		<Compile Update="NullButton.cs">
			<DependentUpon>NullButton.xaml</DependentUpon>
		</Compile>
	</ItemGroup>

	<ItemGroup>
		<Page Update="SplitControl.xaml">
			<XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
			<SubType>Designer</SubType>
		</Page>
		<Page Update="Themes\Generic.xaml">

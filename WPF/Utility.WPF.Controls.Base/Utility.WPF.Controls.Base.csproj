<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net10.0-windows</TargetFramework>
		<Nullable>enable</Nullable>
		<UseWPF>true</UseWPF>
	</PropertyGroup>

	<ItemGroup>
		<ProjectReference Include="..\Utility.WPF.Abstract\Utility.WPF.Abstract.csproj" />
		<ProjectReference Include="..\Utility.WPF.Geometry\Utility.WPF.Geometries.csproj" />
		<ProjectReference Include="..\Utility.WPF.ResourceDictionarys\Utility.WPF.ResourceDictionarys.csproj" />
		<ProjectReference Include="..\Utility.WPF.Helpers\Utility.WPF.Helpers.csproj" />
		<ProjectReference Include="..\Utility.WPF.Panels\Utility.WPF.Panels.csproj" />

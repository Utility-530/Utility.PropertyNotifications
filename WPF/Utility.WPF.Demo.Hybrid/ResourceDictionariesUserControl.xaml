<UserControl
    x:Class="Utility.WPF.Demo.Hybrid.ResourceDictionariesUserControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:mk="http://schemas.utility.com/markup"
    xmlns:view1="http://schemas.utility.com/hybrid"
    xmlns:localvm="clr-namespace:Utility.WPF.Demo.Hybrid.ViewModels"
    xmlns:vm2="clr-namespace:Utility.WPF.Demo.Common.ViewModels;assembly=Utility.WPF.Demo.Common"
    d:DesignHeight="450"
    d:Background="White"
    d:DesignWidth="800"
    mc:Ignorable="d">
     <UniformGrid Columns="1">
        <UniformGrid Rows="1">
            <FrameworkElement.DataContext>
                <localvm:ResourceDictionariesViewModel />
            </FrameworkElement.DataContext>
            <FrameworkElement.Resources>
            </FrameworkElement.Resources>

            <view1:ResourceDictionariesControl 
                mk:NameOfExtension.Type="{x:Type vm2:TickViewModel}"
                DisplayMemberPath="{mk:NameOf Path=Text}"
                CommandPath="{mk:NameOf  Path=Command}"
            IsCheckedPath="{mk:NameOf Path=IsChecked}"
            ItemsSource="{Binding Collection}">
            </view1:ResourceDictionariesControl>
        </UniformGrid>
    </UniformGrid>
</UserControl>
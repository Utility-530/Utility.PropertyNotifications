<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:res="http://schemas.utility.com/resourcedictionary"
                    xmlns:local="clr-namespace:Utility.WPF.Controls.Lists">

    <ResourceDictionary.MergedDictionaries>
        <res:SharedResourceDictionary Source="/Utility.WPF.Controls.Lists;component/Themes/Styles.xaml"></res:SharedResourceDictionary>
        <res:SharedResourceDictionary Source="/Utility.WPF.Themes;component/Themes/CheckBox.xaml"></res:SharedResourceDictionary>
    </ResourceDictionary.MergedDictionaries>

    <Style TargetType="{x:Type local:CheckBoxesListControl}" BasedOn="{StaticResource GenericListBox}">
        <Setter Property="Arrangement" Value="Stack"></Setter>
        <Setter Property="ItemContainerStyle" Value="{StaticResource OtherStyle}" />
        <!--<Setter Property="ItemTemplateSelector" Value="{StaticResource CheckBoxesDataTemplateSelector}" />-->
    </Style>
</ResourceDictionary>
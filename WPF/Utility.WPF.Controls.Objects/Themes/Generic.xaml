    <DataTemplate DataType="{x:Type refl:PropertyInfo}">
        <Grid>
            <TextBlock Text="{Binding Name}" Grid.ColumnSpan="2" />
        </Grid>
    </DataTemplate>
</res:AutoResourceDictionary>
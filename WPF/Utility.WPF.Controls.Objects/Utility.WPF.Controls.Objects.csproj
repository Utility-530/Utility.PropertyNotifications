<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Commands\Utility.Commands\Utility.Commands.csproj" />
    <ProjectReference Include="..\..\Models\Utility.Models\Utility.Models.csproj" />
    <ProjectReference Include="..\Utility.WPF.Controls.Base\Utility.WPF.Controls.Base.csproj" />
    <ProjectReference Include="..\Utility.WPF.Converters\Utility.WPF.Converters.csproj" />
    <ProjectReference Include="..\Utility.WPF.ResourceDictionarys\Utility.WPF.ResourceDictionarys.csproj" />
    <ProjectReference Include="..\Utility.WPF.Demo.Panels\Utility.WPF.Demo.Panels.csproj" />
    <ProjectReference Include="..\Utility.WPF.Templates\Utility.WPF.Templates.csproj" />
    <ProjectReference Include="..\Utility.WPF\Utility.WPF.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="JsonControl.cs">
      <DependentUpon>JsonControl.xaml</DependentUpon>
    </Compile>
    <Compile Update="ObjectControl.cs">
      <DependentUpon>ObjectControl.xaml</DependentUpon>
    </Compile>
    <Compile Update="ObjectFlowControl.cs">
      <DependentUpon>ObjectFlowControl.xaml</DependentUpon>
    </Compile>
    <Compile Update="ObjectItemsControl.cs">
      <DependentUpon>ObjectItemsControl.xaml</DependentUpon>
    </Compile>  
	  <Compile Update="ObjectSelector.cs">
      <DependentUpon>ObjectSelector.xaml</DependentUpon>
    </Compile>
    <Compile Update="ObjectComboBox.cs">
      <DependentUpon>ObjectComboBox.xaml</DependentUpon>
    </Compile>  
		<Compile Update="ObjectListBox.cs">
      <DependentUpon>ObjectListBox.xaml</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <Page Update="JsonControl.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
    </Page>
    <Page Update="ObjectControl.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
      <SubType>Designer</SubType>
    </Page>
    <Page Update="ObjectFlowControl.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
    </Page>
    <Page Update="Themes\Generic.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
      <SubType>Designer</SubType>
    </Page>
  </ItemGroup>

</Project>

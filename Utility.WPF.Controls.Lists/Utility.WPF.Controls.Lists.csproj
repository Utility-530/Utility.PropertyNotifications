<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Utility.WPF.Abstract\Utility.WPF.Abstract.csproj" />
    <ProjectReference Include="..\Utility.WPF.Controls.Base\Utility.WPF.Controls.Base.csproj" />
    <ProjectReference Include="..\Utility.WPF.Controls.Templates\Utility.WPF.Controls.Templates.csproj" />
    <ProjectReference Include="..\Utility.WPF.Markup\Utility.WPF.Markup.csproj" />
    <ProjectReference Include="..\Utility.WPF.Templates\Utility.WPF.Templates.csproj" />
    <ProjectReference Include="..\Utility.WPF.Themes\Utility.WPF.Themes.csproj" />
    <ProjectReference Include="..\Utility.WPF.Helpers\Utility.WPF.Helpers.csproj" />
    <ProjectReference Include="..\Utility.WPF.Controls.Buttons\Utility.WPF.Controls.Buttons.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="ButtonsControl.cs">
      <DependentUpon>ButtonsControl.xaml</DependentUpon>
    </Compile>
    <Compile Update="CheckBoxesComboControl.cs">
      <DependentUpon>CheckBoxesComboControl.xaml</DependentUpon>
    </Compile>
    <Compile Update="CheckBoxesListControl.cs">
      <DependentUpon>CheckBoxesListControl.xaml</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <Page Update="ButtonsControl.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
      <SubType>Designer</SubType>
    </Page>
    <Page Update="CheckBoxesComboControl.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
    </Page>
    <Page Update="CheckBoxesListControl.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
      <SubType>Designer</SubType>
    </Page>
  </ItemGroup>

</Project>
